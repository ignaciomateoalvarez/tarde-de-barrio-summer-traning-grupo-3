<div class='home-container'>
  <div class='home-content-container'>
    <div class='home-header'>
    <div class='home-header-left'>
      <%= link_to '/' do %>
        <p> < </p>
      <% end %>
      <%= link_to '/' do %>
        <p> > </p>
      <% end %>
    </div>
    <div class='home-header-right'>
      <p><%= date %></p>
      <p><%= day %></p>
    </div>
    </div>
    <div class='home-body'>
      <div class='home-body-left'>
        <%= render partial: "home/post_publication_form"%>
        <div class='publications-container'>
          <%= render partial: "home/publications", locals: { publications: @home_presenter.grouped_publications(true), featured: true }%>
          <%= render partial: "home/publications", locals: { publications: @home_presenter.grouped_publications(false), featured: false }%>
        </div>
      </div>
      <div class='home-body-right'>
        <%= render "home/attendance" %>
      </div>
    </div>
  </div>
</div>